modules = ["nodejs-20", "web", "python-3.11"]
[agent]
expertMode = true
integrations = ["stripe:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Dev Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Backend Server"

[[workflows.workflow]]
name = "Dev Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "Backend Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev:server"
waitForPort = 3001

[workflows.workflow.metadata]
outputType = "console"

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 4200

[[ports]]
localPort = 5002
externalPort = 6800

[[ports]]
localPort = 33111
externalPort = 6000

[[ports]]
localPort = 36991
externalPort = 3002

[[ports]]
localPort = 40469
externalPort = 5000

[[ports]]
localPort = 41923
externalPort = 3003

[[ports]]
localPort = 42831
externalPort = 3000

[[ports]]
localPort = 45417
externalPort = 5173

[[ports]]
localPort = 46611
externalPort = 8000

[deployment]
deploymentTarget = "static"
build = ["npm", "run", "build"]
publicDir = "dist"
